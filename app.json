{
  "expo": {
    "name": "Quran App",
    "slug": "quranapp",
    "scheme": "quranapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/App Logo@512.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-screen.png",
