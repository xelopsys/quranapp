        "image": "./assets/splash-screen.png",
        "resizeMode": "cover",
        "dark": {
          "image": "./assets/splash-screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#040C23"
        }
      }
    },
    "web": {
      "bundleer": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6d6e7058-fd4c-414e-9f83-ad8d3e348400"
